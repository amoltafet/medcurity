    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "react-step-progress-bar": "^1.0.3",
    "react-test-renderer": "^17.0.2",
    "redis": "^4.0.3",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "test": "^0.6.0",
    "web-vitals": "^1.1.2",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "react-dotenv && react-scripts start",
    "start-dev": "react-dotenv && react-scripts start",
    "build": "react-dotenv && react-scripts build",
    "test": "react-dotenv && react-scripts test --transformIgnorePatterns 'node_modules/(?!my-library-dir)/'",
    "eject": "react-dotenv && react-scripts eject",
    "clean": "npm cache clean --force",
    "heroku-postbuild": "react-dotenv && npm install && npm run build",
    "server": "npm run dev --prefix server",
    "dev": "concurrently \"npm start\" \"npm run server\"",
    "deploy": "npm install && cd server && npm install"

  },
  "react-dotenv": {
    "whitelist": [
      "BASE_URL",
      "REACT_APP_BASE_URL"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
